    npg:hasRoot subjects:biological-sciences, subjects:health-sciences ;
    npg:id "adaptive-immunity" ;
    npg:isLeaf false ;
    npg:isRoot false ;
    npg:treeDepth 2, 3 ;
    npg:webpage <http://www.nature.com/subjects/adaptive-immunity> ;
    a npg:Subject , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/subjects/> ;
    skos:broader subjects:immunology, subjects:immunopathogenesis ;
    skos:definition "Adaptive immunity is the protection of a host organism from a pathogen or toxin. It is mediated by B cells and T cells, and is characterized by immunological memory. Adaptive immunity is highly specific to a given antigen and is highly adaptable."@en ;
    skos:inScheme <http://ns.nature.com/subjects/> ;
    skos:narrower subjects:cellular-immunity, subjects:class-switch-recombination, subjects:clonal-selection, subjects:humoral-immunity, subjects:immune-tolerance, subjects:somatic-hypermutation, subjects:vdj-recombination ;
    skos:prefLabel "Adaptive immunity"@en .

subjects:adaptive-optics
    npg:hasRoot subjects:physical-sciences ;
    npg:id "adaptive-optics" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 3 ;
    npg:webpage <http://www.nature.com/subjects/adaptive-optics> ;
    a npg:Subject , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/subjects/> ;
    skos:broader subjects:applied-optics ;
    skos:definition "Adaptive optics is a branch of optics and photonics that uses light-controlling components to actively correct distortion in an incoming image. These distortions can occur when light from an astronomical object passes through the Earth’s atmosphere, for example."@en ;
    skos:inScheme <http://ns.nature.com/subjects/> ;
    skos:prefLabel "Adaptive optics"@en .

subjects:adaptive-radiation
    npg:hasRoot subjects:biological-sciences ;
    npg:id "adaptive-radiation" ;
    npg:isLeaf true ;
    npg:isRoot false ;
    npg:treeDepth 3 ;
    npg:webpage <http://www.nature.com/subjects/adaptive-radiation> ;
    a npg:Subject , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/subjects/> ;
    skos:broader subjects:speciation ;
    skos:definition "Adaptive radiation is the process by which a single lineage undergoes multiple speciation events to fill divergent ecological niches. This results in a cluster of phenotypically distinct, related species and often occurs after a physical disturbance or extinction event opens up new niches for exploitation."@en ;
    skos:inScheme <http://ns.nature.com/subjects/> ;
    skos:prefLabel "Adaptive radiation"@en .

