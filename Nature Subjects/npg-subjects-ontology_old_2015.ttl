    a npg:Subject , skos:Concept ;
    rdfs:isDefinedBy <http://ns.nature.com/subjects/> ;
    skos:broader subjects:proteins ;
    skos:definition "Acetyltransferases are enzymes that transfer acetyl (CH3C=O) groups to proteins, generally on lysine residues. This post-translational modification has regulatory roles, notably in chromatin structure and function."@en ;
    skos:inScheme <http://ns.nature.com/subjects/> ;
    skos:prefLabel "Acetyltransferases"@en .

subjects:achalasia
    npg:hasRoot subjects:health-sciences ;
    npg:id "achalasia" ;
    npg:isLeaf true ;
